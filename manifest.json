{
  "ember-modules-codemod": {
    "versions": {
      "ember-cli": "2.16.0-beta.1"
    },
    "projectOptions": ["app", "addon"],
    "nodeVersion": "4.0.0",
    "commands": ["ember-modules-codemod"]
  },
  "ember-qunit-codemod": {
    "versions": {
      "ember-cli": "3.0.0-beta.1"
    },
    "projectOptions": ["app", "addon"],
    "nodeVersion": "4.0.0",
    "commands": ["npx ember-qunit-codemod convert-module-for-to-setup-test ./tests/"]
  },
  "ember-test-helpers-codemod": {
    "versions": {
      "ember-cli": "3.0.0-beta.1"
    },
    "projectOptions": ["app", "addon"],
    "nodeVersion": "6.0.0",
    "commands": [
      "ember-test-helpers-codemod integration tests/integration",
      "ember-test-helpers-codemod acceptance tests/acceptance"
    ]
  },
  "es5-getter-ember-codemod": {
    "versions": {
      "ember-source": "3.1.0-beta.1"
    },
    "projectOptions": ["app", "addon"],
    "nodeVersion": "4.0.0",
    "commands": ["jscodeshift -t https://rawgit.com/rondale-sc/es5-getter-ember-codemod/a2f9fc1c14ba30afc48cb7106ad259eb78dee9b7/es5-getter-ember-codemod.js ./app"]
  },
  "qunit-dom-codemod": {
    "versions": {
      "ember-cli": "3.2.0-beta.1"
    },
    "projectOptions": ["app", "addon"],
    "nodeVersion": "6.0.0",
    "commands": ["jscodeshift -t https://raw.githubusercontent.com/simplabs/qunit-dom-codemod/master/qunit-dom-codemod.js ./tests"]
  }
}
